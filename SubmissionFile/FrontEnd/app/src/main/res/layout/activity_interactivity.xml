<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Interactivity"
        android:background="@drawable/ic_updated_interaction"
        android:id="@+id/test">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            app:autoSizeTextType="none" android:autoText="false"
            android:capitalize="none" android:textSize="34sp" android:textColor="#275F68" android:textStyle="bold"
            android:layout_marginEnd="90dp" android:layout_marginRight="90dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="90dp" android:layout_marginStart="90dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.057" android:minHeight="48dp" android:text="@string/Title"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/TextQuestion"
            android:layout_marginBottom="290dp" android:text="@string/post_question"
            app:layout_constraintVertical_bias="0.0" android:layout_marginTop="290dp"
            android:layout_marginRight="45dp" android:layout_marginLeft="45dp" android:gravity="center_horizontal"
            app:layout_constraintHorizontal_bias="0.0" android:textSize="17sp" android:textColor="#43A3B1"
    />
    <Button
            android:text="@string/bButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button5"
            app:backgroundTint="#00b4d8" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:onClick="BackClickHandler"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.838"
            android:layout_marginEnd="58dp" android:layout_marginRight="58dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.218" android:textSize="13sp"/>
    <Button
            android:text="@string/nButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button3"
            app:backgroundTint="#EF233C" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:onClick="noClickHandler"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.717"
            app:layout_constraintStart_toEndOf="@+id/button" app:layout_constraintHorizontal_bias="0.084"
    />
    <Button
            android:text="@string/yButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:onClick="yesClickHandler" app:backgroundTint="#0BCE83" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.717"
            android:layout_marginLeft="58dp" android:layout_marginStart="58dp"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button4"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:backgroundTint="#023e8a" app:layout_constraintHorizontal_bias="0.153"
            app:layout_constraintVertical_bias="0.957" android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp" android:onClick="loadClickHandler" android:text="@string/lButton"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="1"
            android:ems="2"
            android:id="@+id/UserInputedNodeID"
            android:layout_marginLeft="7dp" android:layout_marginStart="7dp"
            app:layout_constraintStart_toEndOf="@+id/button4" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.957"
            android:minWidth="48dp" android:minHeight="48dp" android:textColor="#023E8A"/>
    <Button
            android:text="@string/mButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2"
            app:backgroundTint="#F5ED00" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:onClick="MaybeClickHandler"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.717"
            app:layout_constraintStart_toEndOf="@+id/button3" app:layout_constraintHorizontal_bias="0.275"
            android:layout_marginRight="5dp" android:layout_marginEnd="5dp"/>
    <TextView
            android:text="@string/pre_description"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/TextDescription"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="475dp"
            app:layout_constraintVertical_bias="0.0" android:layout_marginLeft="45dp"
            android:layout_marginStart="45dp" android:layout_marginRight="45dp" android:layout_marginEnd="45dp"
            android:layout_marginTop="105dp" app:layout_constraintHorizontal_bias="0.0"
            android:gravity="center_horizontal" android:textSize="17sp" android:textColor="#43A3B1"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textGoBackCount"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:textStyle="bold"
            android:textSize="13sp" android:textColor="#327A85"
            android:text="1"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.523"
            app:layout_constraintVertical_bias="0.823"/>
    <TextView
            android:text="@string/GoBackprompt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="130dp"
            android:layout_marginStart="130dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="65dp"
            android:layout_marginRight="65dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.176"
            app:layout_constraintVertical_bias="0.834" android:layout_marginBottom="10dp" android:textStyle="bold"
            android:textColor="#327A85" android:textSize="13sp"/>

</androidx.constraintlayout.widget.ConstraintLayout>